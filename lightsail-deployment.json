{
  "serviceName": "property-valuation-service",
  "containers": {
    "property-valuation-frontend": {
      "image": "IMAGE_PLACEHOLDER_FRONTEND",
      "ports": {
        "80": "HTTP"
      }
    },
    "property-valuation-backend": {
      "image": "IMAGE_PLACEHOLDER_BACKEND",
      "ports": {
        "8080": "HTTP"
      },
      "environment": {
        "SPRING_DATASOURCE_URL": "DATASOURCE_URL",
        "SPRING_DATASOURCE_USERNAME": "DATASOURCE_USERNAME",
        "SPRING_DATASOURCE_PASSWORD": "DATASOURCE_PASSWORD"
      }
    }
  },
  "publicEndpoint": {
    "containerName": "property-valuation-frontend",
    "containerPort": 80,
    "healthCheck": {
      "path": "/",
      "successCodes": "200-499",
      "intervalSeconds": 30,
      "timeoutSeconds": 10,
      "healthyThreshold": 2,
      "unhealthyThreshold": 3
    }
  }
}