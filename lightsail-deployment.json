{
  "serviceName": "property-valuation-service",
  "containers": {
    "property-valuation-frontend": {
      "image": "IMAGE_PLACEHOLDER_FRONTEND",
      "ports": {
        "80": "HTTP"
      }
    },
    "property-valuation-backend": {
      "image": "IMAGE_PLACEHOLDER_BACKEND",
      "ports": {
        "8080": "HTTP"
      },
      "healthCheck": {
        "path": "/actuator/health",
        "healthyThreshold": 2,
        "unhealthyThreshold": 5,
        "timeoutSeconds": 30,
        "intervalSeconds": 35
      }
    }
  },
  "publicEndpoint": {
    "containerName": "property-valuation-frontend",
    "containerPort": 80,
    "healthCheck": {
      "path": "/"
    }
  }
}
